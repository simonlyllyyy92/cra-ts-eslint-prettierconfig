rningCount":0,"source":null},{"filePath":"96","messages":"97","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/simon/Desktop/lint-demo/src/index.tsx",[],"/Users/simon/Desktop/lint-demo/src/App.tsx",[],"/Users/simon/Desktop/lint-demo/src/pages/camera/camera.tsx",[],"/Users/simon/Desktop/lint-demo/src/pages/maps/map.tsx",[],"/Users/simon/Desktop/lint-demo/src/pages/maps/utils/map.connector.ts",[],[],"/Users/simon/Desktop/lint-demo/src/components/setPlacements/setPlacement.component.tsx",[],"/Users/simon/Desktop/lint-demo/src/components/setPlacements/utils/setPlacement.utils.ts",["98","99"],"import { switchMap } from '../../../store/maps/action';\n\nimport { mapConfig } from '../../config/mapconfig';\nimport { MapActionTypes } from '../../../store/types';\n\ninterface MapBorders {\n  width: number;\n  height: number;\n}\ninterface Blocks {\n  [key: number]: { x1: number; x2: number };\n}\n\ninterface PositionProps {\n  x_axis: number;\n  y_axis: number;\n  dispatch: (arg: MapActionTypes) => void;\n  walls: MapBorders;\n  blocks: Array<Blocks>;\n}\ninterface Modifier {\n  [key: string]: { x: number; y: number };\n}\n\nexport const setPlacement = ({ x_axis = 90, y_axis = 34, dispatch, walls, blocks }: PositionProps) => {\n  let x: number = x_axis;\n  let y: number = y_axis;\n  const stepSize = 1;\n  const pixelSize: number = parseInt(getComputedStyle(document.documentElement).getPropertyValue('--pixel-size'));\n  const grillSize: number = 16 * pixelSize;\n\n  const camera_left: number = (160 * pixelSize) / 2 - grillSize / 2; // camera width / 2 - character width / 2\n  const camera_top: number = (144 * pixelSize) / 2 - grillSize / 2; // camera height / 2 - character height / 2\n\n  const modifier: Modifier = {\n    down: { x: 0, y: stepSize },\n    left: { x: -stepSize, y: 0 },\n    right: { x: stepSize, y: 0 },\n    up: { x: 0, y: -stepSize },\n  };\n\n  /**\n   * @OneBox height and width should be 1 * var(--grid-cell)\n   *  it equals to 80px in large Screen as pixelsize = 5px in large screen\n   *  13 box horizontally and 10 box vertically\n   * @Basedon current map height and width\n   *  width: calc(13 * var(--grid-cell));\n   *  height: calc(10 * var(--grid-cell));\n   * @So change the grid-cell value if want to change the map size, don't change the number\n   *  cause number represent the box's number\n   */\n  const { width, height } = walls;\n\n  const leftLimit = 0;\n  const rightLimit: number = (width - 1 * grillSize) / pixelSize;\n  const topLimit = 0;\n  const bottomLimit: number = (height - 2 * grillSize) / pixelSize;\n\n  function step(dir: string): void {\n    const character: HTMLElement | null = document.querySelector('.character');\n    const map: HTMLElement | null = document.querySelector('.map');\n\n    if (dir) {\n      x = x + modifier[dir].x;\n      y = y + modifier[dir].y;\n    }\n    // if steps reach the walls limits then stop\n    if (x < leftLimit) {\n      x = leftLimit;\n    }\n    if (x > rightLimit) {\n      x = rightLimit;\n    }\n    if (y < topLimit) {\n      y = topLimit;\n    }\n    if (y > bottomLimit) {\n      y = bottomLimit;\n      dispatch(switchMap(mapConfig));\n    }\n\n    //  console.log(x, y)\n    if (map === null) {\n      alert('cannot find map element');\n    } else {\n      map.style.transform = `translate3d( ${-x * pixelSize + camera_left}px, ${-y * pixelSize + camera_top}px, 0px )`;\n    }\n\n    if (character === null) {\n      alert('cannot find character element');\n    } else {\n      character.style.transform = `translate3d( ${x * pixelSize}px, ${y * pixelSize}px, 0px )`;\n    }\n  }\n\n  function aiStep() {\n    const ai: HTMLElement | null = document.querySelector('.ai');\n    if (ai === null) {\n      alert('ai element is null');\n    } else {\n      ai.style.transform = `translate3d( ${x * pixelSize}px, ${y * pixelSize}px, 0px )`;\n    }\n  }\n\n  return {\n    step,\n    aiStep,\n  };\n};\n","/Users/simon/Desktop/lint-demo/src/components/setPlacements/utils/connector.ts",[],"/Users/simon/Desktop/lint-demo/src/components/bot/bot.component.tsx",["100","101"],"import * as React from 'react';\nimport './bot.styles.css';\nimport Sprites from '../sprites/sprites.component';\n\ninterface BotProps {\n  sprites: string;\n  step: () => void;\n  direction: string;\n}\n\nconst Bot: React.FunctionComponent<BotProps> = ({ sprites, step, direction }) => {\n  const [dir, setDir] = React.useState(direction);\n  const [walking, setWalking] = React.useState('stop');\n\n  React.useEffect(() => step(), []);\n\n  return (\n    <div className='ai' data-facing={dir} data-walking={walking}>\n      <Sprites sprites={sprites} />\n    </div>\n  );\n};\n\nexport default Bot;\n","/Users/simon/Desktop/lint-demo/src/components/characters/characters.component.tsx",[],"/Users/simon/Desktop/lint-demo/src/components/config/mapconfig.ts",[],"/Users/simon/Desktop/lint-demo/src/store/maps/action.ts",[],"/Users/simon/Desktop/lint-demo/src/components/characters/customizedHooks/useKeyPress.ts",["102","103","104"],"import { useEffect } from 'react';\n\nexport default function useKeyPress(fn: any) {\n  useEffect(() => {\n    document.addEventListener('keydown', fn);\n    document.addEventListener('keyup', fn);\n    return () => {\n      document.removeEventListener('keydown', fn);\n      document.removeEventListener('keyup', fn);\n    };\n  });\n}\n","/Users/simon/Desktop/lint-demo/src/components/characters/helper/character.helper.ts",[],"/Users/simon/Desktop/lint-demo/src/components/sprites/sprites.component.tsx",[],"/Users/simon/Desktop/lint-demo/src/assets/index.js",[],"/Users/simon/Desktop/lint-demo/src/store/types/index.ts",[],"/Users/simon/Desktop/lint-demo/src/store/types/maps/index.ts",[],"/Users/simon/Desktop/lint-demo/src/store/types/maps/map.action.types.ts",[],"/Users/simon/Desktop/lint-demo/src/store/store.ts",["105"],"/Users/simon/Desktop/lint-demo/src/store/root-reducer.ts",[],"/Users/simon/Desktop/lint-demo/src/store/r